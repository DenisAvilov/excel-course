{
    // "parser": "babel-eslint",
    "env": {
       "es6": true,
       "browser": true,
       "node": true 
    },
    "parserOptions": {
        "sourceType": "module",
        // "allowImportExportEverywhere": true,
        "codeFrame": true,
        "ecmaVersion": 8
    },
    "rules": {
       "semi":"off",
       "linebreak-style":"off",
       "arrow-parens":"off",
       "comma-dangle":"off",
       "require-jsdoc":"off",
       "no-undef":"off",
       "operator-linebreak":"off"
    },
    "extends": ["eslint:recommended", "google"]
} 